{
  "auths": {},

  "features": {
    "buildkit": true
  },

  "builder": {
    "gc": {
      "enabled": true,
      "defaultKeepStorage": "20GB"
    }
  },

  "currentContext": "default",
  "detachKeys": "ctrl-p,ctrl-q",

  "psFormat": "table {{.ID}}\\t{{.Image}}\\t{{.Status}}\\t{{.Ports}}\\t{{.Names}}",
  "imagesFormat": "table {{.Repository}}:{{.Tag}}\\t{{.ID}}\\t{{.Size}}",

  "proxies": {
    "default": {}
  }
}
