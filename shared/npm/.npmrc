# NPM Configuration

# Global packages em ~/.npm-global (evita sudo)
prefix=${HOME}/.npm-global

# Salvar vers√£o exata (sem ^ ou ~)
save-exact=true

# Desabilitar mensagens de funding e audit no install
fund=false
audit=false

# Logging
loglevel=warn

# Performance
maxsockets=50

# Scripts
foreground-scripts=true
